# Environment Variables for Bash Scripts Repository
# Copy this file to .env and fill in your actual values

# Hygieia Configuration
HYGIEIA_ENCRYPTOR_PASSWORD=your_secure_password_here
HYGIEIA_DB_USER=dashboarduser
HYGIEIA_DB_PASSWORD=your_database_password_here

# VPN Configuration (for l2p-vpn-setup)
VPN_SERVER_IP=your_vpn_server_ip
VPN_USER=your_username
VPN_PASSWORD=your_password
IPSEC_PSK=your_ipsec_psk

# Apache Virtual Host Configuration
SERVER_ADMIN_EMAIL=admin@example.com
SERVER_NAME=your-server.example.com
PROXY_PORT=8080
